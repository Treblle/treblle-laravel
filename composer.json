{
    "name": "treblle/treblle-laravel",
    "description": "Stay in tune with your APIs",
    "license": "MIT",
    "type": "library",
    "homepage": "https://treblle.com/",
    "keywords": [
        "api",
        "debuging",
        "documentation",
        "laravel",
        "monitoring",
        "treblle"
    ],
    "authors": [
        {
            "role": "Developer",
            "name": "Vedran CindriÄ‡",
            "email": "vedran@treblle.com",
            "homepage": "https://treblle.com/"
        },
        {
            "role": "Developer",
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://treblle.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Treblle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Treblle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "treblle/treblle-sdk-core": "^0.0.2"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "laravel/pint": "^1.6",
        "orchestra/testbench": "^7.0 || ^8.0.8",
        "phpstan/phpstan": "^1.10.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Treblle\\TreblleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
