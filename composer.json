{
    "name": "treblle/treblle-laravel",
    "type": "library",
    "description" : "Stay in tune with your APIs",
    "homepage": "https://treblle.com/",
    "keywords": ["treblle", "api", "monitoring", "loggnig", "debuging", "documentation", "app development"],
    "license": "MIT",
    "require": {
       "php": "^7.2",
        "laravel/framework": "^6.0|^7.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-4" : {
            "Treblle\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Treblle\\TreblleServiceProvider"
            ]
        }
    }
}
