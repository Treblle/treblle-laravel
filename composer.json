{
    "name": "treblle/treblle-laravel",
    "description": "Stay in tune with your APIs",
    "homepage": "https://treblle.com/",
    "type": "library",
    "keywords": [
        "laravel",
        "treblle",
        "api",
        "monitoring",
        "debuging",
        "documentation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vedran CindriÄ‡",
            "email": "vedran@treblle.com",
            "homepage": "https://treblle.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.2.5|^8.0",
        "laravel/framework": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Treblle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Treblle\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Treblle\\TreblleServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
